<!DOCTYPE html>
<html>
  <head>
    <title>SkateboardMarketTracker</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
      

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
      
  </head>

  <body>
   <h1>Skateboard Market Tracker</h1>
      
    <div class="container">
        <div class="row">
            <div class="col-sm-4">
      <button type="button" class="btn btn-outline-danger btn-lg">SCS</button>
            </div>
            
            <div class="col-sm-4">
      <button type="button" class="btn btn-secondary btn-lg">PLB</button>
            </div>
            
            <div class="col-sm-4">
      <button type="button" class="btn btn-secondary btn-lg">AWS</button>
            </div>
        
    </div>
        
        <div class="row">
        
            <div class="col-sm-4">
      <button type="button" class="btn btn-secondary btn-lg">GRL</button>
            </div>
            
            <div class="col-sm-4">
      <button type="button" class="btn btn-secondary btn-lg">SVN</button>
            </div>
            
            <div class="col-sm-4">
      <button type="button" class="btn btn-secondary btn-lg">BLI</button>
            </div>
            
        </div>
    </div>
      
      
 <div id="contact-signup">
      <div class="container">
      
    <h1>Stay Connected</h1>      

    <%= form_for :contact do |e| %>

      <p>
    <%= e.label :first_name %>
    <%= e.text_field :first_name , placeholder:"First Name" %>
          </p>
    
          <p>
  <%= e.label :last_name %>
    <%= e.text_field :last_name , placeholder:"Last Name"%>
      </p>
      
      <p>
  <%= e.label :email %>
    <%= e.text_field :email , placeholder:"Email"%>
      </p>
      
      <p>
    <%= e.submit %>
      </p>
      
    <% end %>
    
          </div>
</div>

      
       </body>
    
</html>

